{
  "rewrites": [
    {
      "source": "/(css|fonts|js)(/.*)?",
      "destination": "/$1$2"
    },
    {
      "source": "/:page/:subpage",
      "destination": "/pages/:page/:subpage.html"
    },
    {
      "source": "/:page",
      "destination": "/pages/:page/:page.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
